{
	"schemaVersion": 1,
	"id": "client_maps",
	"version": "${version}",
	"name": "Client Maps",
	"description": "A mod to add client-side map storage to Minecraft, supporting Map in Slot and Minihud.",
	"authors": [
		"Kroojel",
		"Galiano"
	],
	"contact": {
		"homepage": "https://redstoned.dev",
		"sources": "https://github.com/red-stoned/client_maps",
		"issues": "https://github.com/red-stoned/client_maps/issues"
	},
	"license": "MIT",
	"icon": "assets/client_maps/icon.png",
	"environment": "Client",
	"entrypoints": {
		"client": [
          "com.redstoned.client_maps.ClientMaps"
		]
	},
	"mixins": [
		{
			"config": "client_maps.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.6",
		"java": ">=21",
		"fabric-api": "*"
	}
}
